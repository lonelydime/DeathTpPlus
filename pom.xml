<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.simiancage.deathtpplus</groupId>
	<artifactId>DeathTpPlus</artifactId>
	<version>4.0.0-SNAPSHOT</version>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<repositories>
		<repository>
			<id>bukkit-repo</id>
			<url>http://repo.bukkit.org/content/groups/public/</url>
		</repository>
		<repository>
			<id>spout-repo</id>
			<url>http://repo.spout.org</url>
		</repository>
		<repository>
			<id>vault-repo</id>
			<url>http://ci.herocraftonline.com/plugin/repository/project/Vault/LastSuccessful/repository/</url>
		</repository>
		<repository>
			<id>sk89q-mvn2</id>
			<url>http://maven.sk89q.com/repo</url>
		</repository>
	</repositories>
	<dependencies>
		<dependency>
			<groupId>org.bukkit</groupId>
			<artifactId>bukkit</artifactId>
			<version>1.3.1-R2.0</version>
		</dependency>
		<dependency>
			<groupId>org.getspout</groupId>
			<artifactId>spoutpluginapi</artifactId>
			<version>dev-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>net.milkbowl.vault</groupId>
			<artifactId>Vault</artifactId>
			<version>1.2.19-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.sk89q</groupId>
			<artifactId>worldguard</artifactId>
			<version>5.4</version>
		</dependency>
		<dependency>
			<groupId>com.ensifera.animosity.craftirc</groupId>
			<artifactId>CraftIRC</artifactId>
			<version>3.8</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/CraftIRC.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>org.yi.acru.bukkit.Lockette</groupId>
			<artifactId>Lockette</artifactId>
			<version>1.7.1</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/Lockette.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>com.griefcraft.lwc.LWCPlugin</groupId>
			<artifactId>LWC</artifactId>
			<version>4.2.0</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/LWC.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>com.garbagemule.MobArena</groupId>
			<artifactId>MobArena</artifactId>
			<version>0.94.4.80</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/MobArena.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>org.dynmap</groupId>
			<artifactId>DynmapAPI</artifactId>
			<version>0.50</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/dynmap-api.jar</systemPath>
		</dependency>
	</dependencies>
	<build>
		<resources>
			<resource>
				<targetPath>.</targetPath>
				<filtering>true</filtering>
				<directory>${project.basedir}/Resources/</directory>
				<includes>
					<include>plugin.yml</include>
					<include>config.yml</include>
				</includes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.0.2</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<description>DeathTp, DeathMessages, Tomb and TombStones</description>
	<url>http://dev.bukkit.org/server-mods/deathtpplus/</url>
	<name>DeathTpPlus</name>
	<ciManagement>
		<system>jenkins</system>
		<url>http://ci.bighatchet.com/job/DeathTpPlus/</url>
	</ciManagement>
	<issueManagement>
		<system>GitHub</system>
		<url>https://github.com/dredhorse/DeathTpPlus/issues</url>
	</issueManagement>
	<scm>
		<url>https://github.com/dredhorse/DeathTpPlus</url>
		<connection>scm:git:git://github.com:dredhorse/DeathTpPlus.git</connection>
		<developerConnection>scm:git:git@github.com:dredhorse/DeathTpPlus.git</developerConnection>
	</scm>
</project>
